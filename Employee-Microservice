# Database URL
spring.datasource.url=jdbc:mysql://localhost:3306/employee_db

# Database username and password
spring.datasource.username=root
spring.datasource.password=Bca_b122
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver

# Hibernate properties
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQL8Dialect  # Use MySQL8Dialect if you are using MySQL 8+

# Eureka Client Configuration
eureka.client.serviceUrl.defaultZone=http://localhost:8076/eureka

